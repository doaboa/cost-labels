{
  "manifest_version": 2,
  "name": "Fohr Gmail",
  "version": "0.1",
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*", "http://mail.google.com/*"
      ],
      "js": ["js/jquery-2.2.4.min.js", "js/content.js"],
      "css": ["css/ionicons.css", "css/fohr_gmail.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }

}
